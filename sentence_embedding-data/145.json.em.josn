ate phases following initial infection [65].", "vector": [1.4613605100603309, -0.20437425863201497, 0.6139739556747372, 0.2797634470916819, 0.88790395653632, -0.5520722193905385, 0.39172144588519586, 0.6050142487001722, -0.5850900102195737, -0.3090363429946592, -1.6807843304341077, 0.7233072623494081, -0.8404187956184614, -0.23789663184379606, 1.4510197448507824, 0.9592008980043829, 0.10876108567390474, 0.07853368319410947, -0.5089437971255393, -0.5432305433496367, 0.00426190096186474, 0.14172110280651395, -0.5097210662243015, -2.0685615176334977, -0.4759718803124997, -1.1169131251226645, 1.2963646060670726, 1.302852454391541, 0.536413740075659, -0.7153643439814914, 0.384554700453009, -0.7608838242886122, 0.016541486849746434, -0.10609598612063564, 0.05653036354487995, -0.6951145794300828, 0.9100000055332202, 1.2686124003084842, 0.20269823775743134, -0.43253052329237107, -1.0652516639311216, -0.009203565976349637, -0.803047944282298, 0.5544361753563862, -0.5034599088658069, -0.25652954149700236, 1.1373303525033407, 1.5819980011692678, 0.43200032564709545, 0.671807418751996, -1.180139646083262, -1.0755939042865066, -0.5541687753575388, -0.07617477257736027, -0.17515434496453963, -0.918009960631025, 0.03529132631229004, 0.220071643612755, -0.36185896120150574, -1.1123488164084847, 0.5394085009174887, -0.018636551692907233, -2.0043449160293676, 0.00634340519172838, -0.617149703506584, -0.2297794951882679, -0.6651370729596238, 0.5567027767937702, 0.21605879621347412, 2.3050735367578454, 1.169800703675719, 0.05915553469094448, 0.2160440831357846, 0.438913683174178, 0.4519571766054469, -0.10804492261377163, 2.083763481467031, -0.33342877163522644, -0.5488302079938876, -0.6054983541107504, 0.07100473542232066, 0.20504991506459191, -0.49597337673003494, 0.06947540194960311, -0.8389487328677205, 0.09156055578569067, 1.057559022159694, 1.0052576018424588, -0.3904814992129104, -0.8017293672601227, 0.1599110634342651, 1.492930070264265, -1.0813535826746374, 0.3992050940869376, 