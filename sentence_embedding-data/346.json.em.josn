.", "vector": [0.4079238133472245, 1.8665039086481556, 0.15949794399784878, 0.8532290004077367, 1.1785734401782975, -1.4207456322255894, -0.11592494399519637, 0.5432158089097356, -1.1370177038479596, 0.61861274894909, -1.8756075879209675, 1.4356938354758313, -1.6463132858043537, -0.5108528970813495, 1.1672351840425108, 2.530187201220542, -0.627631729685163, -0.07634037203388289, -0.5392020656727254, 1.2322058981953887, 0.37430198964830197, 0.31270104920440644, -0.10270347975892946, -2.5821090389508754, -0.2829544658306986, -2.0910544431826565, 2.1540251374826767, 0.04565841022190398, 1.2317784777842462, -0.2554697784362361, 0.3923268932849169, -0.8250633446732536, 0.8047584910636942, 1.2836466241860762, -0.6691966240759939, -0.00029922998510301113, 2.4997690623276867, 2.355747298337519, -0.028312476875726134, -0.43270616434165277, -2.8886832431162475, -1.7949988957989262, -0.28350748255616054, 0.047761938636540435, -1.6235254805069417, 0.528690661987639, 0.7737227601246559, 1.0477886354055954, 0.8258622288703918, 0.6911584956978913, -1.166821853723377, -1.2377572942205006, -0.415843756403774, 1.3422074511181563, -2.9247515072347596, -0.21444382018034958, -1.06986185660935, -0.5881115818046965, -0.37730268135419465, -0.12050215825001942, 1.8089993666508235, 0.869580722603132, -1.382301628778805, -1.1270019654475618, -1.2836631568789016, 0.6635452666087076, 2.2918164138682187, -1.6349758335854858, 0.9337817547129816, 1.9378258406650275, 1.0617932176101021, -0.05607866364880465, 0.6578868424549, 0.02118408351088874, -1.048836709735042, -0.457335906161461, 2.073124141403241, -1.8792516467510723, -0.1355473910807632, -2.0111262812097266, 1.254986687854398, 0.8756470833905041, -0.5526265013613738, 1.456857260549441, -1.7695420586387627, 1.2481353734328877, 1.083878055535024, 1.1038322270906065, 0.2531780347926542, -0.1848006287473254, 0.17022856866242364, 1.1428740119445138, -1.0563057070612558, 0.4114639326289762, 0.5011609715002123, 0.9377410503511783, 0.708396599104162