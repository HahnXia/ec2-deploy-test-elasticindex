 scenario.", "vector": [1.2944367245800095, 0.049056306765123736, -0.2572989083419088, 0.5611079333320959, 0.9707730475929566, -0.5414281010453124, -0.7365799454237276, -0.17334965126065072, -1.3667392875795485, -0.36087770163067034, -0.638073000285658, 0.6903069794097973, -0.6801961214077892, -0.588648613764235, 1.1056190694471297, 1.061636117326998, -0.2540091387636494, 0.06668920421725488, -0.7179359431320336, 0.2837129876425024, 0.01615096508703573, -0.3160572183187469, -0.018767598432532395, -1.6667336556129158, -0.4580635576567147, -1.030372772118426, 1.0951066706329584, 0.41335159029404167, -0.12680374876072165, 0.20182209686026908, 0.43077964749681996, -0.5919496116475784, 0.06080445196130313, 1.1543411556485808, -0.036433388475415995, -0.40565514522313606, 1.87793763022637, 1.3986646631965414, -0.21118178520919173, 0.369624812155962, -1.5543206314905547, -0.07559937977930531, -0.9058109326724662, 1.3613597279181704, -0.6682394424278755, 0.15196442586602643, 0.61379515840963, 1.2871945865481393, 0.2671130478847772, 0.8879108331166208, -1.192253766244903, -1.319247942097718, -0.13919699109101202, -0.10579745736322366, 0.027330738375894725, -0.8903443715535104, 0.17317816808645148, 0.9206302422790031, -1.1279378289236774, -0.6120231729728403, 1.2569357326428872, -0.12870788093277952, -1.5616978101315908, -0.40384187661402393, 0.04386220750893699, 0.9509592918911949, 0.24596628303334, -0.9774951744911959, 0.612553548773576, 0.9304334686603397, 0.6110459638875909, 0.7492000141210156, 0.9685700687550707, -0.25769497139845043, -0.45601744029772817, -0.9946434538759377, 1.1957879630936077, -1.2235164551602793, 0.1616110635077348, -0.706179350061575, -0.20627990103093907, 0.11229454276326578, 0.2546369388583116, 0.12722211371874437, -0.21885196967923548, 0.47350871840899345, 1.2259700380382128, 0.3198402316465945, -0.12109991640318185, -0.37742906536732335, 0.7453420435631415, 1.3086276948397426, -0.472471565939486, 0.055311188829364255, 0.2951338460279658, -0.15515