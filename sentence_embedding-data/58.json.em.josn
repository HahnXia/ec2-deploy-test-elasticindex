logical conditions and may thus be more informative of what actually happens in \u201cthe real world.\u201d", "vector": [0.8746273783035576, -0.24054780098231276, 0.10240197655366501, 0.7647398404660635, 0.9159436316476786, -0.3635063048131997, -0.31841783809068147, -0.2185536409415363, -1.6688104163622484, -0.7423051123623736, -1.0622335870866664, 0.5081890918663703, -0.27508660467356094, -0.3524578127398854, 0.8375935940564432, 0.8552112190518528, -0.39172545140900183, 0.24765462955110706, -0.6407854915596545, 0.3292306366492994, -0.025668735805083998, -0.5905708507898453, -0.0953666132845683, -1.3215864439844154, -0.45682139467680827, -0.4477216530358419, 0.9324349943708512, 0.43332928557174455, 0.041637663376604905, -0.07722686036140658, 0.2086535241396632, -0.12347447736829054, 0.0859763857733924, 0.7061383348191157, 0.25425554694083985, -0.39596946467918315, 1.367270833812654, 1.289109178178478, 0.2467728566571168, -0.21480504233113606, -1.3696036964975065, -0.016926430311286822, -0.5960482782393228, 0.5502690625216928, -0.2810495289013488, -0.14537020228453912, 0.7646759969356935, 1.0290706476880587, 0.15433661502902396, 0.6456691740349925, -1.2930531784368213, -1.1676015389402892, -0.23016994760837406, -0.22088861162774265, -0.3198344319825992, -0.7407502190180821, 0.15270987950498238, 1.009167215670459, -0.5732254945323803, -0.5988047982827993, 0.8787809467175975, -0.024666812449140707, -1.154957224498503, -0.059959150064969435, -0.3489522667223355, 0.5710539248830173, 0.3766538064519409, -0.3466903964872472, 0.5733421577360787, 0.8618902048074233, 0.5715380217006896, 0.058387496468640165, 0.6169626852351939, -0.20251564976933878, -0.4887194247166917, -0.7415038621238637, 0.8391073404782219, -0.4374044913347461, 0.23441893188282847, -0.7096280269033741, -0.047419405317214114, 0.26087710066349246, -0.0498431806336157, 0.1763120611103659, -0.1141719730803743, 0.6167659948259825, 0.9765338949509896, 0.242390020408493, 0.19119302332910593, -0.21160664079798153,